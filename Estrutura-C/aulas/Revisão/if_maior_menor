#include <stdio.h>
#include <stdlib.h>

int main(void) {

int numero1, numero2;
printf("Digite o primeiro numero: ");
scanf("%d", &numero1);
printf("\nDigite o segundo numero: ");
scanf("%d", &numero2);

if (numero1 > numero2 ){
  printf("\nO numero %d é maior que %d", numero1, numero2);
}else if (numero2 > numero1) {
  printf("\nO numero %d é maior que %d", numero2, numero1);}
else{
  printf("\nO numero %d e %d são iguais", numero2, numero1);}
  
 return 0;
}